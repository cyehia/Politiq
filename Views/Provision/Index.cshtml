@model IEnumerable<Politiq.Models.ObjectModel.Provision>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Article
        </th>
        <th>
            Text
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Article)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Text)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ProvisionID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ProvisionID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ProvisionID })
        </td>
    </tr>
}

</table>
